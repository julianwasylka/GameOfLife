<Application x:Class="GameOfLife.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GameOfLife.WPF"
             xmlns:converters="clr-namespace:GameOfLife.WPF.Converters"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <converters:CellPositionConverter x:Key="CellPositionConverter" />
        <converters:BoardSizeToPixelsConverter x:Key="BoardSizeToPixelsConverter" />
    </Application.Resources>
</Application>
